@model IEnumerable<CarMVCClasses.ApiPhone>

@{
    ViewBag.Title = "Phones";
}

<h2>Phones</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PhoneId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PhoneNumber)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PhoneId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PhoneNumber)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new {  id=item.PhoneId  }) |
            @Html.ActionLink("Details", "Details", new {  id=item.PhoneId  }) |
            @Html.ActionLink("Delete", "Delete", new {  id=item.PhoneId  })
        </td>
    </tr>
}

</table>
